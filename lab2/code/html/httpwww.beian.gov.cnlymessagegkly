stData(pageno, isInitPagination) {\r\n        var pagesize = 10;\r\n        $.ajax({\r\n            type: "post",\r\n            url: "../lymessage/gkly/query",\r\n            scriptCharset: \'utf-8\',\r\n            async: false,\r\n            contentType: "application/json",\r\n            data: JSON.stringify({\r\n                "pageno" : pageno,\r\n                "pagesize":pagesize,\r\n                "keyword" : $("#keyword").val()\r\n            }),\r\n            success: function(result){\r\n                var html = "";\r\n                $(\'.topic-list\').html(\'\');\r\n                for (var i = 0; i < result.listData.length; i++) {\r\n                    var item = result.listData[i];\r\n                    var createtime = item.createtime;\r\n\r\n                    if (typeof createtime == \'number\') {\r\n                        createtime = dayjs(createtime).format(\'YYYY-MM-DD HH:mm:ss\');\r\n                    }\r\n\r\n                    html += \'<li><a title="\' + item.title + \'" href="#" onclick="handleLyDetail(\\\'\' + item.id + \'\\\')">\' + item.title + \'</a><span>\' + createtime + \'</span>\'\r\n                        + \'</li>\';\r\n                }\r\n                $("#topic-list").html(html);\r\n\r\n                if (isInitPagination) {\r\n                    new Pagination({\r\n                        element: \'#pagination\',\r\n                        type: 1,\r\n                        pageIndex: result.currentpage,\r\n                        pageSize: pagesize,\r\n                        pageCount: 5,\r\n                        total: result.total,\r\n                        jumper: true,\r\n                        singlePageHide: false,\r\n                        prevText: \'\xe4\xb8\x8a\xe4\xb8\x80\xe9\xa1\xb5\',\r\n                        nextText: \'\xe4\xb8\x8b\xe4\xb8\x80\xe9\xa1\xb5\',\r\n                        disabled: true,\r\n                        currentChange: function(index) {\r\n                            getListData(index, false);\r\n                        }\r\n                    });\r\n                }\r\n            }\r\n        });\r\n    }\r\n\r\n    /**\r\n     * \xe6\x9f\xa5\xe7\x9c\x8b\xe7\x95\x99\xe8\xa8\x80\xe8\xaf\xa6\xe6\x83\x85\r\n     * @param id\r\n     */\r\n    function handleLyDetail(id) {\r\n        var layer = layui.layer\r\n\r\n        $.ajax({\r\n            method: \'get\',\r\n            url: "/lymessage/gkly/locate",\r\n            data: {\r\n                id: id,\r\n            },\r\n            scriptCharset: \'utf-8\',\r\n            success: function(result) {\r\n                if (result) {\r\n                    var rowsHtml = \'\'\r\n\r\n                    if (typeof result.createtime == \'number\') {\r\n                        result.createtime = dayjs(result.createtime).format(\'YYYY-MM-DD HH:mm:ss\');\r\n                    }\r\n                    if (typeof result.hftime == \'number\') {\r\n                        result.hftime = dayjs(result.hftime).format(\'YYYY-MM-DD HH:mm:ss\');\r\n                    }\r\n\r\n                    rowsHtml = \'<tr><td><strong>\xe6\xa0\x87\xe9\xa2\x98</strong></td><td>\' + (result.title || \'\') + \'</td></tr>\'\r\n                                + \'<tr><td><strong>\xe5\x86\x85\xe5\xae\xb9</strong></td><td>\' + result.content + \'</td></tr>\'\r\n                                + \'<tr><td><strong>\xe7\x95\x99\xe8\xa8\x80\xe6\x97\xb6\xe9\x97\xb4</strong></td><td>\' + result.createtime + \'</td></tr>\'\r\n                                + \'<tr><td><strong>\xe5\x9b\x9e\xe5\xa4\x8d\xe6\x97\xb6\xe9\x97\xb4</strong></td><td>\' + (result.hftime || \'\') + \'</td></tr>\'\r\n                                + \'<tr><td><strong>\xe5\x9b\x9e\xe5\xa4\x8d\xe5\x86\x85\xe5\xae\xb9</strong></td><td>\' + (result.hfcontent || \'\') + \'</td></tr>\'\r\n                                ;\r\n\r\n                    $(\'#ly-detail tbody\').html(rowsHtml);\r\n                    layer.open({\r\n                        type: 1,\r\n                        title: \'\xe7\x95\x99\xe8\xa8\x80\xe8\xaf\xa6\xe6\x83\x85\',\r\n                        area: \'700px\',\r\n                        content: $(\'#ly-detail\')\r\n                    })\r\n                }\r\n            },\r\n            error: function(err) {\r\n                console.error(err)\r\n            }\r\n        });\r\n    }\r\n\r\n    $(function() {\r\n        getListData(1, true);\r\n    });\r\n</script>\r\n</html>'