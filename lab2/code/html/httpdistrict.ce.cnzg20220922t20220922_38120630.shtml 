("<span style=\\"font-size:12px; color:#000000\\">\xb9\xb2(<font color=\\"#FF0000\\">");\n\t  document.write(_nPageCount);\n\t  document.write("<span style=\\"font-size:12px; color:#000000\\">)\xd2\xb3</span>&nbsp;");\n\t  //var fyn;\n\t var nStep=5\n\t  var nCurrIndex = _nCurrIndex || 0;\n\t  \tif(nCurrIndex < 0)return;\n\t\n\t  if(nCurrIndex > 0)\n\t  \tdocument.write("<a href=\\""+_sPageName+"."+_sPageExt+"\\" style=font-size:12px color:#0000FF target=_top>\xca\xd7 \xd2\xb3</a>&nbsp;");\n\t  else\n\t    document.write("<span style=\\"font-size:12px; color:#000000\\">\xca\xd7 \xd2\xb3</span>&nbsp;&nbsp;");\n\t\t\n\t  if(nCurrIndex > 0){\n\t      if(nCurrIndex == 1){\n\t\t  document.write("<a href=\\""+_sPageName+"."+_sPageExt+"\\" target=_top>\xc9\xcf\xd2\xbb\xd2\xb3</a>&nbsp;&nbsp;");\n\t\t  }else{document.write("<a href=\\""+_sPageName+"_" + (nCurrIndex-1) + "."+_sPageExt+"\\" target=_top>\xc9\xcf\xd2\xbb\xd2\xb3</a>&nbsp;&nbsp;");}\n\t    }\n\t  else{\n\t    document.write("<span style=\\"font-size:12px; color:#000000\\">\xc9\xcf\xd2\xbb\xd2\xb3</span>&nbsp;&nbsp;");\n\t    }\n\t    /*if(nCurrIndex == 0)\n\t\tdocument.write("1&nbsp;");\n\telse\n\t\tdocument.write("<a href=\\""+_sPageName+"."+_sPageExt+"\\">1</a>&nbsp;");*/\n\n//\xca\xd7\xd2\xb3\xba\xcd\xc9\xcf\xd2\xbb\xd2\xb3\xd2\xd4\xbc\xb0\xb5\xda\xd2\xbb\xd2\xb3\xb5\xc4\xb4\xfa\xc2\xeb\n\n\n\n\n\tif(_nPageCount<=4){//_nPageCount\xd0\xa1\xd3\xda\xb5\xc8\xd3\xda4\xb5\xc4\xc7\xe9\xbf\xf6\n\tif(nCurrIndex == 0)\n\t\tdocument.write("1&nbsp;");\n\telse\n\t\tdocument.write("<a href=\\""+_sPageName+"."+_sPageExt+"\\" target=_top>1</a>&nbsp;");\n\tfor(var i=1; i<_nPageCount; i++){\n\t\t\tif(nCurrIndex == i)\n\t\t\t\tdocument.write((i+1) + "&nbsp;");\n\t\t\telse\n\t\t\t\tdocument.write("<a href=\\""+_sPageName+"_" + i + "."+_sPageExt+"\\" target=_top>"+(i+1)+"</a>&nbsp;");\n\t\t}\n\n\n\t}else{//_nPageCount\xd0\xa1\xd3\xda\xb5\xc8\xd3\xda4\xb5\xc4\xc7\xe9\xbf\xf6end\n\t//\xcf\xc2\xc3\xe6\xd5\xe2\xb8\xf6IF\xca\xc7\xc5\x