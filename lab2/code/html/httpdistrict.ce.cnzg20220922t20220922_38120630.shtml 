var ssssss=Math.ceil(nStep/2);\n\t   //alert("\xb2\xbd\xb3\xa4\xd2\xbb\xb0\xeb\xb5\xc4\xca\xfd\xd6\xb511:  "+ssssss+" nCurrIndex11: "+nCurrIndex);\n\t\t  //if(i > 0 && i <= _nPageCount){\n\t\t  if(i > 0 && i <_nPageCount){\n\t\t\t  if(i==nCurrIndex)\n\t    \t\tdocument.write("<span style=\\"color:#000;\\">[</span><span style=\\"color:#FF0000; font-size:12px\\" target=_top>"+(i+1)+"</span><span style=\\"color:#000;\\">]</span>&nbsp;&nbsp;");\n\t\t\t  else\n\t    \t\tdocument.write("<span style=\\"color:#000;\\">[</span><a href=\\""+_sPageName+"_" + i + "."+_sPageExt+"\\" target=_top>"+(i+1)+"</a><span style=\\"color:#000;\\">]</span>&nbsp;&nbsp;");\n\t\t  }\n\t\t  \n\t  }\t  \n\t } \n\t \n\n\t //\xcf\xc2\xd2\xbb\xd2\xb3\xba\xcd\xc4\xa9\xd2\xb3\xb5\xc4\xc9\xe8\xd6\xc3\n\t   var nextOver = _nPageCount - nCurrIndex;\t//\xb7\xad\xd2\xb3\xb5\xbd\xbc\xab\xcf\xde\xca\xb1\xba\xf2;\n\t   if(nCurrIndex < (_nPageCount-1))\n\t   document.write("<a href=\\""+_sPageName+"_" + (nCurrIndex+1) + "."+_sPageExt+"\\" target=_top>\xcf\xc2\xd2\xbb\xd2\xb3</a>&nbsp;&nbsp;");\t \n\t  \telse if (nextOver <= nStep){\n\t    document.write("<span style=\\"font-size:12px; color:#000000\\" target=_top>\xcf\xc2\xd2\xbb\xd2\xb3</span>&nbsp;&nbsp;");}\t\n      \n\t  \n\t  if(nCurrIndex != (_nPageCount-1))\n\t     document.write("<a href=\\""+_sPageName+"_" + (_nPageCount-1) + "."+_sPageExt+"\\" target=_top>\xc4\xa9 \xd2\xb3</a>&nbsp;&nbsp;");\t\n\t\telse\n\t    document.write("<span style=\\"font-size:12px; color:#000000\\">\xc4\xa9 \xd2\xb3</span>");\n\t  //\xcf\xc2\xd2\xbb\xd2\xb3\xba\xcd\xc4\xa9\xd2\xb3\xb5\xc4\xc9\xe8\xd6\xc3\t\n\n\n\n}//\xba\xaf\xca\xfd\xbd\xe1\xca\xf8\xb7\xfb\n//WCM\xd6\xc3\xb1\xea\ncreatePageHTML(1, 0, "t20220922_38120630", "shtml");\t\n//-->\n          </SCRIPT>\n\n\n\n</div>\n\n<!--\xbe\xab\xb2\xca\xcd\xbc\xc6\xac\xcd\xc6\xbc\xf6\xb5\xf7\xd3\xc3-->\n\n<style>\n.aneixia{width:915px; border:1px solid #DBDBDB; margin:0 auto; margin-top:10px; padding-bottom:10px;}\n.aneixia_b{w