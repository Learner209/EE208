\t\t\t\t}\n\t\t\t\t\t\tfunction setJFInfo(d, i, limit, $w){\n\t\t\t\t\t\t\tvar temp = d[i];\n\t\t\t\t\t\t\tif(!d[i]||i>=limit) return;\n\t\t\t\t\t\t\tif(i==0){\n\t\t\t\t\t\t\t\t\n\t\t\t\t\t\t\t\t$.ajax({\n\t\t\t\t\t\t\t\t\turl: "//api.sports.sina.com.cn/?p=sports&s=sport_client&a=index&_sport_t_=football&_sport_s_=opta&_sport_a_=getteam&id=" + temp.sl_id + "&dpc=1",\n\t\t\t\t\t\t\t\t\ttype: "GET",\n\t\t\t\t\t\t\t\t\tdataType: "jsonp",\n\t\t\t\t\t\t\t\t\tdata:{\n\t\t\t\t\t\t\t\t\t\tdpc: 1\n\t\t\t\t\t\t\t\t\t},\n\t\t\t\t\t\t\t\t\tcache:true,\n\t\t\t\t\t\t\t\t\tjsonpCallback:\'CB_F6BA8554_ABF1_47A8_BF51_E803E9D3C663_\'+ temp.sl_id,\n\t\t\t\t\t\t\t\t\tsuccess: function(data){\n\t\t\t\t\t\t\t\t\t\t$w.append(\'<li class="hvr"><span class="rk_order">\'+temp.team_order+\'</span><span class="jf_team"><a href="http://match.sports.sina.com.cn/football/team.php?id=\'+temp.sl_id+\'&l_type=\'+ temp.l_type +\'" target="_blank"><img src="\'+data.result.data.logo_small.replace(\'http://\', \'//\')+\'" alt="\'+temp.team_cn+\'" />\'+temp.team_cn+\'</a></span><span class="jf_count">\'+temp.count+\'</span><span class="rk_result">\'+temp.score+\'</span></li>\');\n\t\t\t\t\t\t\t\t\t\ti++;\n\t\t\t\t\t\t\t\t\t\tsetJFInfo(d, i, limit, $w);\n\t\t\t\t\t\t\t\t\t},\n\t\t\t\t\t\t\t\t\terror:function(){\n\t\t\t\t\t\t\t\t\t\twindow.console && window.console.log("error");\n\t\t\t\t\t\t\t\t\t}\n\t\t\t\t\t\t\t\t})\n\t\t\t\t\t\t\t} else{\n\t\t\t\t\t\t\t\t$w.append(\'<li><span class="rk_order">\'+temp.team_order+\'</span><span class="jf_team"><a href="http://match.sports.sina.com.cn/football/team.php?id=\'+temp.sl_id+\'&l_type=\'+ temp.l_type +\'" target="_blank">\'+temp.team_cn+\'</a></span><span class="jf_count">\'+temp.count+\'</span><span class="rk_result">\'+temp.score+\'</span></li>\');\n\t\t\t\t\t\t\t\ti++;\n\t\t\t\t\t\t\t\tsetJFInfo(d, i, limit, $w);\n\t\t\t\t\t\t\t}\n\t\t\t\t\t\t}\n\t\t\t\t\t\t// \xe7\xa7\xaf\xe5\x88\x86\xe6\xa6\x9c\xe5\x88\x9d\xe5\xa7\x8b\xe5\x8c\x96\xef\xbc\x8c\xe5\x8f\x82\xe6\x95\xb0\xe8\xa1\xa8\xe7\xa4\xba\xe5\x8f\xaf\xe5\xb1\x95\xe7\xa4\xba\xe6\x9d\xa1\xe6\x95\xb0\xef\xbc\x8c\xe5\x8f\xaf\xe9\x85\x8d\xe7\xbd\xae\n\t\t\t\t\t\tinitJF(7);\n\t\t\t\t\t\t// \xe5\xb0\x84\xe6\x89\x8b\xe6\xa6\x9c\xe5\x88\x9d\xe5\xa7\x8b\xe5\x8c\x96\n\t\t\t\t\t\tfunction initShs(season, limit){\n\t\t\t\t\t\t\t\n\t\t\t\t\t\t\t$.ajax({\n\t\t\t\t\t\t\t\turl: "//api.sports.sina.com.cn/?p=sports&s=sport_client&a=index&_sport_t_=football&_sport_s_=opta&_sport_a_=playerorder&item=13&type=4&season=" + season + "&limit=" + limit + "&dpc=1",\n\t\t\t\t\t\t\t\ttype: "GET",\n\t\t\t\t\t\t\t\tdataType: "jsonp",\n\t\t\t\t\t\t\t\tdata:{\n\t\t\t\t\t\t\t\t\tdpc: 1\n\t\t\t\t\t\t\t\t},\n\t\t\t\t\t\t\t\tcache:true,\n\t\t\t\t\t\t\t\tjsonpCallback:\'CB_F6BA8554_ABF1_47A8_BF51_E803E9D3C661_\'+[season, limit].join(\'_\').replace(/-/g,\'_\'),\n\t\t\t\t\t\t\t\tsuccess: function(data){\n\t\t\t\t\t\t\t\t\tvar dataList = data.result.data, len = dataList.length;\n\t\t\t\t\t\t\t\t\tfor(var i=0; i<len; i++){\n\t\t\t\t\t\t\t\t\t\tvar singleData = dataList[i];\n\t\t\t\t\t\t\t\t\t\t$(".scorer_rank .rk_list ul").append(\'<li><span class="rk_order">\'+singleData.num+\'</span><span class="sc_team"><a href="http://match.sports.sina.com.cn/football/player.php?id=\'+singleData.player_id+\'" target="_blank">\'+singleData.player_name+\'</a> | <a href="http://match.sports.sina.com.cn/football/team.php?id=\'+singleData.sl_team_id+\'&l_type=\'+ singleData.l_type +\'" target="_blank">\'+singleData.team_name+\'</a></span><span class="rk_result">\'+singleData.item1+\'</span></li>\');\n\t\t\t\t\t\t\t\t\t}\n\t\t\t\t\t\t\t\t},\n\t\t\t\t\t\t\t\terror: function(){\n\t\t\t\t\t\t\t\t\twindow.console && window.console.log("error");\n\t\t\t\t\t\t\t\t}\n\t\t\t\t\t\t\t})\n\t\t\t\t\t\t}\n\t\t\t\t\t\t// \xe5\xb0\x84\xe6\x89\x8b\xe6\xa6\x9c\xe5\x88\x9d\xe5\xa7\x8b\xe5\x8c\x96\xef\xbc\x8c\xe5\x8f\x82\xe6\x95\xb0\xe8\xa1\xa8\xe7\xa4\xbaseason\xe5\x92\x8climit\xef\xbc\x8c\xe5\x8f\xaf\xe9\x85\x8d\xe7\xbd\xae\n\t\t\t\t\t\tinitShs(2022, 7);\n\t\t\t\t\t\t// \xe6\xa6\x9c\xe5\x8d\x95\xe5\x88\x87\xe6\x8d\xa2\n\t\t\t\t\t\t$(".rk_tabs span").click(function(){\n\t\t\t\t\t\t\tvar $this = $(this), index = $this.index();\n\t\t\t\t\t\t\t$this.addClass("rk_tabs_on").siblings().removeClass("rk_tabs_on");\n\t\t\t\t\t\t\t$(".rk_conts .rk_cont").eq(index).show().siblings().hide();\n\t\t\t\t\t\t})\n\t\t\t\t\t\t// \xe6\xbb\x91\xe8\xbf\x87\xe6\x95\x88\xe6\x9e\x9c\n\t\t\t\t\t\t$(".rk_list").delegate("li:not(\'.rk_list_title\')","mouseenter",function(){\n\t\t\t\t\t\t\tvar $this = $(this), $img = $this.find(".jf_team img");\n\t\t\t\t\t\t\t$this.addClass("hvr").siblings().removeClass("hvr");\t\t\t\t\t\t\t\n\t\t\t\t\t\t})\n\t\t\t\t\t})\n\t\t\t\t\t</script>\n\t\t\t\t\n\t\t\t\t</div>\n\t\t\t\t<!-- \xe8\xb0\x81\xe6\x98\xafMVP begin -->\n\t\t\t\t<div class="sec_blk mrg_b_20" data-sudaclick="blk_premierleague_sdyd">\n\t\t\t\t\t\n<div class="sec_blk mrg_b_20" data-sudaclick="blk_premierleague_sdyd"style="display:none;">\n\t<div class="sec_blk_top">\n            <a href="http://sports.sina.com.cn/g/pl/season1516mvp/" target="_blank" class="sec_blk_title">\xe8\x8b\xb1\xe8\xb6\x85MVP\xe7\xa5\xa8\xe9\x80\x89</a>\n        </div>\n\t<div class="article_blk mrg_b_10">\n\t\t<a href="http://sports.sina.com.cn/g/pl/season1516mvp/" target="_blank">\n\t\t\t<img src="//n.sinaimg.cn/sports/mvp_vote/mvp.jpg" alt="2015-16\xe8\xb5\x9b\xe5\xad\xa3\xe8\x8b\xb1\xe8\xb6\x85\xe8\x81\x94\xe8\xb5\x9bMVP\xe6\x8a\x95\xe7\xa5\xa8" height="150" width="300">\n\t\t</a>\n\t</div>\n</div>\n\t\t\t\t</div>\n\t\t\t\t<!-- \xe8\xb0\x81\xe6\x98\xafMVP end -->\n\t\t\t\t<!-- \xe6\xb7\xb1\xe5\xba\xa6\xe9\x98\x85\xe8\xaf\xbb begin -->\n\t\t\t\t<div class="sec_blk mrg_b_20" data-sudaclick="blk_premierleague_sdyd">\n\t\t\t\t\t<div class="sec_blk_top"><a href="http://roll.sports.sina.com.cn/sportsori/index.shtml" target="_blank" class="sec_blk_title">\xe6\xb7\xb1\xe5\xba\xa6\xe9\x98\x85\xe8\xaf\xbb</a><a class="sec_blk_more" href="http://roll.sports.sina.com.cn/spo