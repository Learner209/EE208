\t\tsuccess: function(r){\n\t\t\t\t\t\tvar dataList = r.result.data;\n\t\t\t\t\t\tfor(key in dataList){\n\t\t\t\t\t\t\tif(key>=10) break;\n\t\t\t\t\t\t\tvar tempData = dataList[key];\n\t\t\t\t\t\t\t$contestList.append("<li></li>");\n\t\t\t\t\t\t\tvar $contestLi = $contestList.find("li").eq(key);\n\t\t\t\t\t\t\t$contestLi.append(\'<p class="cst_time">\'+tempData.date.replace(/^\\d+-/,\'\')+\' \'+tempData.time+\'</p>\')\n\t\t\t\t\t\t\tvar status = tempData.match_status, cls = "", mtLink = "", linkHtml = "";\n\t\t\t\t\t\t\tif(status == "3"){ //\xe5\xae\x8c\xe8\xb5\x9b\n\t\t\t\t\t\t\t\tcls ="over";\n\t\t\t\t\t\t\t\tmtLink = tempData.NewsUrl;\n\t\t\t\t\t\t\t\ttempData.VideoUrl&&(linkHtml+=\'<a href="\'+tempData.VideoUrl+\'" target="_blank" class="mrg_r_5">\xe8\xa7\x86\xe9\xa2\x91\xe9\x9b\x86\xe9\x94\xa6</a>\');\n\t\t\t\t\t\t\t\ttempData.ImgUrl&&(linkHtml+=\'<a href="\'+tempData.ImgUrl+\'" target="_blank">\xe5\x9b\xbe\xe9\x9b\x86</a>\');\n\t\t\t\t\t\t\t} else if(status == "2"){ //\xe6\xaf\x94\xe8\xb5\x9b\xe4\xb8\xad\n\t\t\t\t\t\t\t\tcls ="on";\n\t\t\t\t\t\t\t\tmtLink = tempData.LiveUrl;\n\t\t\t\t\t\t\t\t\n\t\t\t\t\t\t\t\tvideoliveurl = tempData.VideoLiveUrl;\n\t\t\t\t\t\t\t\t\n\t\t\t\t\t\t\t\tif(videoliveurl.indexOf(\'smgbb.cn\') != -1){\n\t\t\t\t\t\t\t\t\tvar datetime=new Date();\n\t\t\t\t\t\t\t\t\tvar s=datetime.getFullYear()+""+(datetime.getMonth()+1)+""+datetime.getDate()+""+datetime.getHours()+""+datetime.getMinutes()+""+datetime.getSeconds();\n\t\t\t\t\t\t\t\t\tvideoliveurl = videoliveurl + \'?source=sina&subsource=premierleagueIndex&tokenId=\'+s;\n\t\t\t\t\t\t\t\t}\n\t\t\t\t\t\t\t\t\n\t\t\t\t\t\t\t\tvideoliveurl&&(linkHtml+=\'<a href="\'+videoliveurl+\'" target="_blank" class="mrg_r_5">\xe8\xa7\x86\xe9\xa2\x91\xe7\x9b\xb4\xe6\x92\xad</a>\');\n\t\t\t\t\t\t\t\ttempData.LiveUrl&&(linkHtml+=\'<a href="\'+tempData.LiveUrl+\'" target="_blank">\xe5\x9b\xbe\xe6\x96\x87\xe7\x9b\xb4\xe6\x92\xad</a>\');\n\t\t\t\t\t\t\t} else if(status == "1"){  //\xe6\x9c\xaa\xe5\xbc\x80\xe8\